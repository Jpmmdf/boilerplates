$TTL 2d    ; default TTL for zone
$ORIGIN internal.jpit.com.br. ; base domain-name

@         IN      SOA   ns.internal.jpit.com.br. info.jpit.com.br. (
                                 2024010300 ; serial number
                                 12h        ; refresh
                                 15m        ; update retry
                                 3w         ; expiry
                                 2h         ; minimum TTL
                                )
           IN       NS   ns.internal.jpit.com.br.

ns         IN       A    192.168.0.10

*          IN       A    192.168.0.10

srv-1      IN       A    192.168.0.10
srv-2      IN       A    192.168.0.21
srv-3      IN       A    192.168.0.22
srv-4      IN       A    192.168.0.31
srv-5      IN       A    192.168.0.32
srv-6      IN       A    192.168.0.33
srv-7      IN       A    192.168.0.34
;
